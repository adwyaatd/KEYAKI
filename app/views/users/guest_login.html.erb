<div class="main users-new">
  <div class="container">
    <div class="form-heading">ゲストユーザーでログインしますか？</div>
    <div class="form users-form">
      <div class="form-body">
        <% if @error_message %>
          <div class="form-error">
            <%= @error_message %>
          </div>
        <% end %>
        <%= form_tag("/login") do%>
          <p>メールアドレス</p>
          <input name="email" value="guestuser@gmail.com">
          <p>パスワード</p>
          <input name="password" type="password" value="guestuser">
          <input type="submit" value="ログイン">
        <% end %>
      </div>
    </div>
  </div>
</div>